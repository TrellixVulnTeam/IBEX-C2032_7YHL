   "source": [
    "network = Network()\n",
    "train_loader= torch.utils.data.DataLoader(train_set,batch_size=batch_size)\n",
    "optimizer = torch.optim.Adam(network.parameters(),lr=learning_rate)\n",
    "\n",
    "comment = f' batch_size={batch_size} lr={learning_rate}'"
   ]
