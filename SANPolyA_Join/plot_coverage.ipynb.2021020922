    "#!/usr/bin/env Rscript\n",
    "library(ggplot2)\n",
    "library(ggpubr)\n",
    "library(ggrepel)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "plotPASCoverage <- function(pas){\n",
    "    if(is.numeric(pas)){\n",
    "        df =  data.frame(position = 1:176,coverage = as.vector(as.matrix(data[pas,9:184])))\n",
    "    }\n",
    "    else{\n",
    "        cov = subset(data,data$V1 == pas)[,9:184]\n",
    "        df = data.frame(position = 1:176,coverage = as.vector(as.matrix(cov)))\n",
    "    }\n",
    "    print(pas)\n",
    "    p = ggplot(data=df,aes(x=position,y=coverage))+\n",
    "    geom_line()+\n",
    "    labs(title=paste(\"Coverage of PAS \",pas,sep=''))+\n",
    "    theme_bw()+\n",
    "    theme(axis.title=element_text(size=12),\n",
    "          axis.text=element_text(size=12,vjust=0.5,hjust=0.5,angle=0),\n",
    "          legend.title=element_blank(),\n",
    "          plot.title = element_text(size=15,face = \"bold\",hjust = 0.5),\n",
    "          legend.text=element_text(size=11,color=\"black\",face=\"bold\"))\n",
    "    print(p)\n",
    "}"
   ]
  },
